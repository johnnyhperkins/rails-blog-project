<h1>Home Page</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        
      </tr>
    <% end %>
  </tbody>

  <tbody>
    <% @skills.each do |skill| %>
      <tr>
        <td><%= skill.title %></td>
        <td><%= skill.percent_utilized %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
